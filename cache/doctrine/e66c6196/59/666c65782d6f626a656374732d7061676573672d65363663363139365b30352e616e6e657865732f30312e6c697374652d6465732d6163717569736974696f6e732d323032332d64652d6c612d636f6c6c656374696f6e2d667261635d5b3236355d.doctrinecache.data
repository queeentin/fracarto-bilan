        content: "<div class=\"artsheet\">\n    <p class=\"number\">1</p>\n    <p class=\"author\">Pierre Unal-Brunet</p>\n    <p class=\"title\">\"A Ghost fattened by a glossy necromass, 2022</p>\n    <p class=\"description\">Toile de jute sur châssis aluminium, encre, acrylique, gesso, impressions jet d'encre, bindex, papier recyclé, peinture aérosol, pigments, guanine</p>\n    <p class=\"size\">170 x 120 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">2</p>\n    <p class=\"author\">Pierre Unal-Brunet</p>\n    <p class=\"title\">\"Arapaïma Shoes, 2022</p>\n    <p class=\"description\">Toile de jute sur châssis aluminium, encre, acrylique, gesso, impressions jet d'encre, bindex, papier recyclé, peinture aérosol, pigments, guanine</p>\n    <p class=\"size\">170 x 120 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">3</p>\n    <p class=\"author\">Pierre Unal-Brunet</p>\n    <p class=\"title\">\"Hugging dermal denticules, 2022</p>\n    <p class=\"description\">Toile de jute sur châssis aluminium, encre, acrylique, gesso, impressions jet d'encre, bindex, papier recyclé, peinture aérosol, pigments, guanine</p>\n    <p class=\"size\">170 x 120 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">4</p>\n    <p class=\"author\">Jean-Baptiste Janisset</p>\n    <p class=\"title\">\"Vers l'infini et au-delà, 2022</p>\n    <p class=\"description\">Plastique, plomb, étain, zinc, plexiglass</p>\n    <p class=\"size\">150 x 120 x 90 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">5</p>\n    <p class=\"author\">Jean-Baptiste Janisset</p>\n    <p class=\"title\">\"La lumière pour les ancêtres, 2022</p>\n    <p class=\"description\">Plomb</p>\n    <p class=\"size\">201 x 33 x 5 cm</p>\n    <p class=\"price\">Don de l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">6</p>\n    <p class=\"author\">Janna Zhiri</p>\n    <p class=\"title\">\"Manifest-sourire, les vâches-rose en dalmatien</p>\n    <p class=\"description\">Installation composée de dessins, sculptures et son : pastel sec, peinture acrylique, encre, plâtre et filasse</p>\n    <p class=\"size\">1000 x 160 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">7</p>\n    <p class=\"author\">Céline Vaché-Olivieri</p>\n    <p class=\"title\">\"Seeing double, 2023</p>\n    <p class=\"description\">Cartons, huile de lin, scotch, colle latex, papier, plexiglass et acier. (40 pièces + table)</p>\n    <p class=\"size\">190 x 80 x 600 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Stéphanie Cherpin</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">8</p>\n    <p class=\"author\">Camille Lavaud</p>\n    <p class=\"title\">\"Les réclames du Consortium des prairies, 2021</p>\n    <p class=\"description\">Acrylique, encres aquarellables, encre de chine sur toile</p>\n    <p class=\"size\">format gauche : ? x 545cm<br>format droit : ? x 600 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Camille de Singly</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">9</p>\n    <p class=\"author\">Sarah Benslimane</p>\n    <p class=\"title\">\"Tic Tac Toe, 2022</p>\n    <p class=\"description\">Guirlandes de lierre, papiers hollographiques, papiers peints, adhésifs pour miroirs, perles, laque glycéro, acrylique, structure en bois</p>\n    <p class=\"size\">245 x 205 cm</p>\n    <p class=\"price\">Achat à la galerie Madragoa, Lisbonne</p>\n    <p class=\"proposition\">Proposition de Paul Bernard</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">10</p>\n    <p class=\"author\">Andreas Hochuli</p>\n    <p class=\"title\">\"Workshop, 2016</p>\n    <p class=\"description\">Acrylique sur toile</p>\n    <p class=\"size\">80 x 120 cm</p>\n    <p class=\"price\">Achat à l’artiste</p>\n    <p class=\"proposition\">Proposition de Paul Bernard</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">11</p>\n    <p class=\"author\">Andreas Hochuli</p>\n    <p class=\"title\">\"Le sous-bois, 2016</p>\n    <p class=\"description\">Acrylique sur toile</p>\n    <p class=\"size\">100 x 140 cm</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Paul Bernard</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">12</p>\n    <p class=\"author\">Delphine Gigoux-Martin</p>\n    <p class=\"title\">\"L’arrière-pays, 2015</p>\n    <p class=\"description\">Taxidermie de renard, bois, plastique</p>\n    <p class=\"size\">Dimensions variables</p>\n    <p class=\"price\">Achat à la galerie Gastaud</p>\n    <p class=\"proposition\">Proposition de Yannick Miloux</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">13</p>\n    <p class=\"author\">Jean-Simon Raclot</p>\n    <p class=\"title\">\"Sans titre</p>\n    <p class=\"description\">Huile sur toile</p>\n    <p class=\"size\">110 x 110 x 2 cm</p>\n    <p class=\"price\">Achat à la galerie Emmanuel Hervé, Marseille</p>\n    <p class=\"proposition\">Proposition de Yannick Miloux</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">14</p>\n    <p class=\"author\">Jean-Simon Raclot</p>\n    <p class=\"title\">\"Sans titre</p>\n    <p class=\"description\">Huile sur toile</p>\n    <p class=\"size\">27 x 35 x 2 cm</p>\n    <p class=\"price\">Achat à la galerie Emmanuel Hervé, Marseille</p>\n    <p class=\"proposition\">Proposition de Yannick Miloux</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">15</p>\n    <p class=\"author\">Jean-Simon Raclot</p>\n    <p class=\"title\">\"Sans titre</p>\n    <p class=\"description\">Huile sur toile</p>\n    <p class=\"size\">54 x 73 x 2 cm</p>\n    <p class=\"price\">Achat à la galerie Emmanuel Hervé, Marseille</p>\n    <p class=\"proposition\">Proposition de Yannick Miloux</p>\n</div>\n\n<div class=\"artsheet\">\n    <p class=\"number\">16</p>\n    <p class=\"author\">Nelly Monnier</p>\n    <p class=\"title\">\"Vendémiaire, Brumaire, Frimaire, Nivôse, Pluviôse, Ventôse, Germinal, Floréal, Prairial, Messidor, Thermidor, Fructidor de la série Calendrier révolutionnaire</p>\n    <p class=\"description\">12 peintures à l’huile sur toile</p>\n    <p class=\"size\">116 x 89 cm (chaque)</p>\n    <p class=\"price\">Achat à l'artiste</p>\n    <p class=\"proposition\">Proposition de Yannick Miloux</p>\n</div>"
