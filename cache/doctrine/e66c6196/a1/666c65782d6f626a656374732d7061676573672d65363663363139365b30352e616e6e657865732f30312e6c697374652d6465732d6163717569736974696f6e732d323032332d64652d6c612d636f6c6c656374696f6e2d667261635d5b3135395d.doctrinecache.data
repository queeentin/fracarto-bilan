        content: "<div class=\"artsheet\">\n    <p class=\"number\">1</p>\n    <p class=\"author\">Pauline&nbsp;BARZILA&Iuml;</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1987, &agrave; Boulogne-Billancourt</p>\n    <p class=\"place\">Vit et travaille &agrave; Marseille</p>\n    <p class=\"title\"><i>Retour au calme</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Splendorlux 350&nbsp;g, 7&nbsp;passages couleur</p>\n    <p class=\"size\">70 &times; 100&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec La&nbsp;Fraternelle, Saint-Claude</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">2</p>\n    <p class=\"author\">Tamaris&nbsp;BORRELLY</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1987, &agrave; Paris</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris</p>\n    <p class=\"title\"><i>Symbiose</i>, 2023</p>\n    <p class=\"description\">Eau forte sur papier Cor&eacute;en GPK 02 28-32&nbsp;g, 5&nbsp;passages couleur</p>\n    <p class=\"size\">66 &times; 96&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;Atelier&nbsp;Ren&eacute;&nbsp;Taz&eacute;, Paris</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">3</p>\n    <p class=\"author\">Io&nbsp;BURGARD</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1987, &agrave; Talence</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris et Aubervilliers</p>\n    <p class=\"title\"><i>La Jambe au lever</i>, 2023</p>\n    <p class=\"description\">Lithographie sur papier Velin BFK Rives 300&nbsp;g, 3&nbsp;passages couleur et rehaut de pastel</p>\n    <p class=\"size\">120 &times; 80&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;Atelier&nbsp;Michael&nbsp;Woolworth, Paris</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">4</p>\n    <p class=\"author\">Alix&nbsp;DELMAS</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1962, &agrave; Bayonne</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris et &agrave; Talais</p>\n    <p class=\"title\"><i>Paysage forestier et personnages</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Arches Aquarelle 365&nbsp;g, 3&nbsp;passages couleur</p>\n    <p class=\"size\">79,3 &times; 113,5&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec Tchikebe, Marseille</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">5</p>\n    <p class=\"author\">Leah&nbsp;DESMOUSSEAUX</strong></p>\n    <p class=\"birth\">N&eacute;e en 1995, &agrave; Cahors</p>\n    <p class=\"place\">Vit et travaille &agrave; Escamps</p>\n    <p class=\"title\"><i>Vision 1</i>, 2023</p>\n    <p class=\"description\">H&eacute;liogravure sur papier Hahnem&uuml;hle 755&nbsp;300&nbsp;g, 1&nbsp;passage couleur</p>\n    <p class=\"size\">70 &times; 99&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;atelier&nbsp;H&eacute;liog, Meudon</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">6</p>\n    <p class=\"author\">Vanessa&nbsp;DZIUBA</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1982, &agrave; Rueil-Malmaison</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris et Ivry-sur-Seine</p>\n    <p class=\"title\"><i>Print&eacute; \\(ᴖᴗᴖ)/ &eacute;temps \\(ᴖᴗᴖ)/ auver \\(ᴖᴗᴖ)/ hitomne</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Opal Joop Stop, 300&nbsp;g, 5&nbsp;passages couleur&nbsp;; eau forte et aquatinte sur papier Velin d&rsquo;Arches 160&nbsp;g (15,5 &times; 27,5&nbsp;cm chacune)&nbsp;; impression risographique sur papier Munken Pure Rough 120&nbsp;g (20 &times; 15,5&nbsp;cm) ; gaufrage</p>\n    <p class=\"size\">70 &times; 100&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;Institut s&eacute;rigraphique, l&rsquo;Atelier R.L.D. et le studio Fid&egrave;le, Paris</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">7</p>\n    <p class=\"author\">Juliette&nbsp;GREEN</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1995, &agrave; Semur-en-Auxois</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris</p>\n    <p class=\"title\"><i>Qu&rsquo;est-ce que les g&eacute;n&eacute;rations futures penseront de nous ?</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Velin BFK Rives 270&nbsp;g, 3&nbsp;passages couleur</p>\n    <p class=\"size\">107 &times; 75 cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec Tchikebe, Marseille</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">8</p>\n    <p class=\"author\">Seulgi&nbsp;LEE</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1972, &agrave; S&eacute;oul</p>\n    <p class=\"place\">Vit et travaille &agrave; Bagnolet</p>\n    <p class=\"title\"><i>Tenon</i>, 2023</p>\n    <p class=\"description\">Pochoir sur papier Velin BFK Rives 270&nbsp;g, 26&nbsp;passages couleur</p>\n    <p class=\"size\">75 &times; 107&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec L&rsquo;Estampe&nbsp;moderne, Paris</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">9</p>\n    <p class=\"author\">Diego&nbsp;MOVILLA</strong></p>\n    <p class=\"birth\">N&eacute; en&nbsp;1974, &agrave; Burgos</p>\n    <p class=\"place\">Vit et travaille &agrave; Tours et &agrave; Saint-Pierre-des-Corps</p>\n    <p class=\"title\"><i>Le Jugement de P&acirc;ris, d&rsquo;apr&egrave;s Claude Gell&eacute;e dit Le Lorrain</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Old Mill Eco 40&nbsp;350&nbsp;g, 3&nbsp;passages couleur</p>\n    <p class=\"size\">70 &times;100&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;atelier&nbsp;&Eacute;cran&nbsp;total, Saint-Pierre-des-Corps</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">10</p>\n    <p class=\"author\">Paul&nbsp;POUVREAU</strong></p>\n    <p class=\"birth\">N&eacute; en&nbsp;1956, &agrave; Aulnay-sous-Bois</p>\n    <p class=\"place\">Vit et travaille &agrave; Montreuil et &agrave; Argenton-sur-Creuse</p>\n    <p class=\"title\"><i>Les Permanentes</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Arena Natural Rough 310&nbsp;g, 6&nbsp;passages couleur</p>\n    <p class=\"size\">92 &times; 68&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec La&nbsp;Presse&nbsp;Pur&eacute;e, Rennes</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">11</p>\n    <p class=\"author\">SUPER TERRAIN</strong></p>\n    <p class=\"birth\">Super Terrain (Quentin&nbsp;Bodin, Luc&nbsp;de&nbsp;Fouquet et Lucas&nbsp;Mayer) a &eacute;t&eacute; cr&eacute;&eacute; en&nbsp;2004.</p>\n    <p class=\"place\">Travaille &agrave; Nantes et Marseille</p>\n    <p class=\"title\"><i>Montagne de feu</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Fabriano 100&nbsp;% Cotton Velluto 240&nbsp;g, 7&nbsp;passages couleur</p>\n    <p class=\"size\">98 &times; 68&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec L&eacute;zard&nbsp;graphique, Brumath, et Mistral&nbsp;Graphic, Carcassonne</p>\n</div>\n<div class=\"artsheet\">\n    <p class=\"number\">12</p>\n    <p class=\"author\">Agn&egrave;s&nbsp;THURNAUER</strong></p>\n    <p class=\"birth\">N&eacute;e en&nbsp;1962, &agrave; Paris</p>\n    <p class=\"place\">Vit et travaille &agrave; Paris</p>\n    <p class=\"title\"><i>Po&egrave;me</i>, 2023</p>\n    <p class=\"description\">S&eacute;rigraphie sur papier Velin BFK Rives 300&nbsp;g, 4&nbsp;passages couleur</p>\n    <p class=\"size\">90 &times; 110&nbsp;cm</p>\n    <p class=\"collab\">&OElig;uvre r&eacute;alis&eacute;e en collaboration avec l&rsquo;Atelier&nbsp;Arcay, Paris</p>\n</div>"
